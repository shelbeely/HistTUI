{
  "name": "histtui",
  "version": "1.1.0",
  "description": "Interactive Git History TUI - Explore repository history with a beautiful terminal interface",
  "main": "dist/index.js",
  "bin": {
    "histtui": "./dist/cli.js"
  },
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target bun --format esm --define 'process.env.NODE_ENV=\"development\"'",
    "start": "bun dist/cli.js",
    "dev": "bun --watch src/cli.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "git",
    "tui",
    "terminal",
    "history",
    "cli",
    "ink"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "bun": ">=1.3.5"
  },
  "type": "module",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.12",
    "@ai-sdk/openai": "^3.0.9",
    "@anthropic-ai/sdk": "^0.71.2",
    "@inkjs/ui": "^2.0.0",
    "ai": "^6.0.30",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "execa": "^9.6.1",
    "fuse.js": "^7.1.0",
    "ink": "^6.6.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^5.0.0",
    "ink-markdown": "^1.0.4",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-table": "^3.1.0",
    "ink-text-input": "^6.0.0",
    "marked": "^9.1.6",
    "minimatch": "^10.1.1",
    "nanoid": "^5.1.6",
    "openai": "^6.16.0",
    "ora": "^9.0.0",
    "parse-diff": "^0.11.1",
    "patch-console": "^2.0.0",
    "react": "^19.2.3",
    "react-devtools-core": "^6.1.5",
    "simple-git": "^3.30.0",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/marked": "^5.0.2",
    "@types/node": "^25.0.6",
    "@types/react": "^19.2.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
